{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes":[
    {
      "src": "/api/(.*)", 
      "methods": ["GET", "POST", "PUT", "DELETE"], 
      "dest": "/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      } 
    },
    {
      "src": "/week/(.*)",
      "methods": ["GET"],
      "dest": "/week/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/"
    }
  ]
}