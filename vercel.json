{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes":[
    {
      "src": "/api/(.*)", 
      "methods": ["GET", "POST", "PUT", "DELETE"], 
      "headers": {
        "Access-Control-Allow-Origin": "*" 
      },
      "dest": "/" 
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}